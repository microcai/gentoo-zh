+INT32 FsReleaseCache(struct super_block *sb)
+{
+	FS_INFO_T *p_fs = &(EXFAT_SB(sb)->fs_info);
+
+	sm_P(&(fs_struct[p_fs->drv].v_sem));
+
+	FAT_release_all(sb);
